<?xml version="1.0" encoding="UTF-8"?>
<suite name="Effort NXT Automation">
	<parameter name="browser" value="Chrome" />
	<parameter name="siteURL"
		value="https://secure.spoors.io/effortx/login" />

	<!--<parameter name="siteURL" value="https://staging.spoors.in/effortx/login" />-->

	<!-- <parameter name="siteURL" value="testurl" /> -->

	<parameter name="headless" value="false" />
	<listeners>
		<listener class-name="com.effort.nxt.util.TestListener"></listener>
	</listeners>

	<test name="EmployeeFilters">
		<classes>
			<class name="com.effort.nxt.test.filters.EmployeeFiltersTest" />
		</classes>
	</test>

	<test name="EntitiesFilters">
		<classes>
			<class name="com.effort.nxt.test.filters.EntityFiltersTest" />
		</classes>
	</test>

	<test name="LeavesFilters">
		<classes>
			<class name="com.effort.nxt.test.filters.LeavesFiltersTest" />
		</classes>
	</test>

</suite>