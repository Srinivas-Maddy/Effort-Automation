<?xml version="1.0" encoding="UTF-8"?>
<suite name="Effort NXT Automation">
	<parameter name="browser" value="Chrome" />
	<parameter name="siteURL"
		value="https://secure.spoors.io/effortx/login" />

	<!-- <parameter name="siteURL" value="testurl" /> -->

	<listeners>
		<listener class-name="com.effort.nxt.util.TestListener"></listener>
	</listeners>

	<test name="Login">
		<classes>
			<class name="com.effort.nxt.test.LoginTest" />
		</classes>
	</test>
	

	<test name="employee">
		<classes>
			<class
				name="com.effort.nxt.test.employees.EmployeeCreationTest" />
		</classes>
	</test>
	

	<test name="works">
		<classes>
			<class name="com.effort.nxt.test.works.WorkProcessCreationTest" />
			<!-- <class name="com.effort.nxt.test.works.AddWorkProcessTest" /> -->
		</classes>
	</test>
	

	<test name="forms">
		<classes>
			<class name="com.effort.test.forms.FormsTest" />
			<class name="com.effort.test.forms.FormSubmissionTest" />
		</classes>
	</test>
	
	<test name="Export Functionalities">
		<classes>
			<class name="com.effort.nxt.test.exports.EmployeeExports" />
		</classes>
	</test>
	
	<test name="Web Logins">
	<classes>
			<class name="com.effort.nxt.test.login.WebAppLoginTest" />
		</classes>
	</test>


</suite>